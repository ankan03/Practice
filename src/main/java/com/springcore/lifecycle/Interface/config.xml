<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
  	
	<!-- If we  want to create initialize and destroy function using interface then we need not do anything in config file explecitely -->
	<bean id="pizza" class="com.springcore.lifecycle.Interface.Pizza">  
		<property name="price" value="60"></property>
	</bean>
	
	<bean id="berger" class="com.springcore.lifecycle.Interface.Berger">  
		<property name="price" value="90"></property>
	</bean>
	
	
</beans>  